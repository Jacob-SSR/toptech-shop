{
  "framework": "nuxtjs",
  "buildCommand": "npm ci && npm run build",
  "installCommand": "npm ci",
  "outputDirectory": ".output",
  "functions": {
    "api/**.js": {
      "runtime": "nodejs18.x"
    },
    "api/**.mjs": {
      "runtime": "nodejs18.x"
    },
    "chunks/**.mjs": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ]
}
