{
  "framework": "nuxtjs",
  "buildCommand": "npm ci && npm run build",
  "installCommand": "npm ci",
  "outputDirectory": ".output",
  "functions": {
    "api/**": {
      "runtime": "vercel-nodejs@3.0.0"
    },
    "chunks/**": {
      "runtime": "vercel-nodejs@3.0.0"
    },
    "server/**": {
      "runtime": "vercel-nodejs@3.0.0"
    }
  },
  "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ]
}
